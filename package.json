{
  "name": "node-poweredup",
  "version": "8.0.5",
  "description": "A Javascript module to interface with LEGO Powered Up components.",
  "homepage": "https://github.com/nathankellenicki/node-poweredup/",
  "main": "dist/node/index-node.js",
  "type": "module",
  "types": "dist/node/index-node.d.ts",
  "scripts": {
    "all": "pnpm run build && pnpm run docs",
    "build": "pnpm run lint && pnpm run build:all",
    "build:all": "pnpm run build:node && pnpm run build:browser",
    "build:browser": "vite build src",
    "build:node": "swc ./**/*.ts -d dist/node",
    "dev": "vite serve src",
    "docs": "jsdoc -c .jsdoc.json",
    "lint": "eslint \"./src/**/*.ts\"",
    "prepublishOnly": "pnpm run lint && pnpm run build:node"
  },
  "author": "Nathan Kellenicki <nathan@kellenicki.com>",
  "license": "MIT",
  "dependencies": {
    "@abandonware/noble": "1.9.2-21",
    "compare-versions": "^6.0.0",
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.69",
    "@types/debug": "4.1.8",
    "@types/node": "^20.4.2",
    "@types/web-bluetooth": "0.0.17",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "better-docs": "^2.7.2",
    "buffer": "^6.0.3",
    "eslint": "^8.44.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0",
    "prettier": "^3.0.0",
    "taffydb": "^2.7.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.3",
    "vite-plugin-html": "^3.2.0"
  }
}
